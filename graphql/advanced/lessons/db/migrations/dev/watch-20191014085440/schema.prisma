datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
  output   = "../node_modules/@generated/photon"
}

generator photon {
  provider = "photonjs"
}

model Class {
  id      Int @id
  lessons ClassLesson[]
}

model ClassLesson {
  id            Int      @id
  title         String   @unique
  description   String?
  class         Class
}